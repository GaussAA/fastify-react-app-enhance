# API 项目环境配置模板
# 此文件是模板，实际配置由脚本生成

NODE_ENV={{NODE_ENV}}
PORT={{PORT}}

# 数据库配置
DATABASE_URL=postgresql://{{DB_USER}}:{{DB_PASSWORD}}@{{DB_HOST}}:{{DB_PORT}}/{{DB_NAME}}
REDIS_URL=redis://{{REDIS_HOST}}:{{REDIS_PORT}}

# JWT 配置
JWT_SECRET={{JWT_SECRET}}
JWT_EXPIRES_IN="7d"

# CORS 配置
CORS_ORIGIN=http://localhost:{{WEB_PORT}}

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/api.log

# API 密钥
API_KEY={{API_KEY}}